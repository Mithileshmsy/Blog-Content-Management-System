<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>SiteManger</servlet-name>
        <servlet-class>web.SiteManger</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>RegisterAPI</servlet-name>
        <servlet-class>web.RegisterAPI</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LoginAPI</servlet-name>
        <servlet-class>web.LoginAPI</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>DeleteAPI</servlet-name>
        <servlet-class>web.DeleteAPI</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>UpdateAPI</servlet-name>
        <servlet-class>web.UpdateAPI</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertAPI</servlet-name>
        <servlet-class>web.InsertAPI</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>InsertServletAPI</servlet-name>
        <servlet-class>web.InsertServletAPI</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>LogoutAPI</servlet-name>
        <servlet-class>web.LogoutAPI</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>CategoryInsertAPI</servlet-name>
        <servlet-class>web.CategoryInsertAPI</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>EnquiryInsertAPI</servlet-name>
        <servlet-class>web.EnquiryInsertAPI</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>SiteManger</servlet-name>
        <url-pattern>/SiteManger</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>RegisterAPI</servlet-name>
        <url-pattern>/RegisterAPI</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LoginAPI</servlet-name>
        <url-pattern>/LoginAPI</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>DeleteAPI</servlet-name>
        <url-pattern>/DeleteAPI</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>UpdateAPI</servlet-name>
        <url-pattern>/UpdateAPI</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertAPI</servlet-name>
        <url-pattern>/InsertAPI</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>InsertServletAPI</servlet-name>
        <url-pattern>/InsertServletAPI</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>LogoutAPI</servlet-name>
        <url-pattern>/LogoutAPI</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>CategoryInsertAPI</servlet-name>
        <url-pattern>/CategoryInsertAPI</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>EnquiryInsertAPI</servlet-name>
        <url-pattern>/EnquiryInsertAPI</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
